<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="wwwroot\js\site.js" />
      <_ContentIncludedByDefault Remove="Areas\Userpanel\Views\Home\Index.cshtml" />
      <_ContentIncludedByDefault Remove="Areas\Userpanel\Views\_ViewImports.cshtml" />
      <_ContentIncludedByDefault Remove="Areas\Userpanel\Views\_ViewStart.cshtml" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="wwwroot\theme-assets\.DS_Store" />
      <Content Include="wwwroot\theme-assets\images\.DS_Store" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="8.0.0" />
    </ItemGroup>

</Project>
